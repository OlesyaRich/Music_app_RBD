<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="547.0" prefWidth="1467.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.olesya.musicaldevapp.controller.RequirementTypeController">
    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
        <TableView fx:id="requirementTypesTable" prefHeight="299.0" prefWidth="1447.0">
            <columns>
                <TableColumn fx:id="requirementTypeIdRequirementTypesTable" prefWidth="202.20000302791595" text="ID типа требования" />
                <TableColumn fx:id="requirementTypeNameRequirementTypesTable" minWidth="0.0" prefWidth="206.79998779296875" text="Имя типа требования" />
            </columns>
        </TableView>
        <VBox layoutX="-1.0" layoutY="299.0" prefHeight="246.0" prefWidth="1466.0">
            <children>
                <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0">
                    <children>
                        <Label text="Поиск">
                            <HBox.margin>
                                <Insets left="20.0" right="10.0" />
                            </HBox.margin>
                            <font>
                                <Font name="System Bold" size="12.0" />
                            </font>
                        </Label>
                        <Label text="ID типа требования:">
                            <padding>
                                <Insets right="10.0" />
                            </padding>
                        </Label>
                        <TextField fx:id="requirementTypeIdRequirementTypeTextField" prefHeight="26.0" prefWidth="270.0" promptText="Введите ID типа требования (UUID)..." />
                        <Label layoutX="77.0" layoutY="52.0" text="Имя типа:">
                            <padding>
                                <Insets left="10.0" right="10.0" />
                            </padding>
                        </Label>
                        <TextField fx:id="requirementTypeNameRequirementTypeTextField" layoutX="170.0" layoutY="47.0" prefHeight="26.0" prefWidth="270.0" promptText="Введите имя типа (Строка)..." />
                        <Button fx:id="requirementTypeFilterButton" mnemonicParsing="false" text="Фильтровать">
                            <HBox.margin>
                                <Insets left="100.0" />
                            </HBox.margin></Button>
                    </children>
                </HBox>
                <HBox alignment="CENTER_LEFT" prefHeight="80.0" prefWidth="1466.0">
                    <children>
                        <Label text="Изменение">
                            <HBox.margin>
                                <Insets left="20.0" right="10.0" />
                            </HBox.margin>
                            <font>
                                <Font name="System Bold" size="12.0" />
                            </font>
                        </Label>
                        <VBox prefHeight="84.0" prefWidth="1366.0">
                            <children>
                                <HBox alignment="CENTER_LEFT" prefHeight="78.0" prefWidth="1366.0">
                                    <children>
                                        <Label text="ID типа требования:">
                                            <HBox.margin>
                                                <Insets right="10.0" />
                                            </HBox.margin>
                                        </Label>
                                        <TextField fx:id="requirementTypeIdInputField" editable="false" prefHeight="26.0" prefWidth="202.0" promptText="Заполняется автоматически" style="-fx-background-color: e6e6e6;">
                                            <HBox.margin>
                                                <Insets right="10.0" />
                                            </HBox.margin></TextField>
                                        <Label layoutX="10.0" layoutY="28.0" text="Имя типа требования:">
                                            <HBox.margin>
                                                <Insets right="10.0" />
                                            </HBox.margin>
                                        </Label>
                                        <TextField fx:id="requirementTypeNameInputField" prefHeight="26.0" prefWidth="238.0" promptText="Введите имя типа требования (Строка)...">
                                            <HBox.margin>
                                                <Insets right="10.0" />
                                            </HBox.margin>
                                        </TextField>
                                    </children>
                                </HBox>
                            </children>
                        </VBox>
                    </children>
                </HBox>
                <HBox alignment="CENTER" prefHeight="41.0" prefWidth="1466.0">
                    <children>
                        <Button fx:id="saveChangesButton" disable="true" mnemonicParsing="false" text="Изменить">
                            <HBox.margin>
                                <Insets right="10.0" />
                            </HBox.margin>
                        </Button>
                        <Button fx:id="deleteButton" disable="true" layoutX="703.0" layoutY="18.0" mnemonicParsing="false" text="Удалить">
                            <HBox.margin>
                                <Insets right="10.0" />
                            </HBox.margin>
                        </Button>
                        <Button fx:id="addButton" layoutX="748.0" layoutY="18.0" mnemonicParsing="false" text="Добавить">
                     <HBox.margin>
                        <Insets right="10.0" />
                     </HBox.margin></Button>
                  <Button fx:id="stopSelectionButton" mnemonicParsing="false" text="Снять выделение" />
                    </children>
                </HBox>
            </children>
        </VBox>
    </AnchorPane>
</AnchorPane>
